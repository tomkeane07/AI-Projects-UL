    "def getCol(M, i):\n",
    "    if i > len(M[0]):\n",
    "        raise Exception(\"out of bounds\")\n",
    "    else:\n",
    "        C = []\n",
    "        for x in range(len(M)):\n",
    "            C.append(M[x][i])\n",
    "        return C"
   ]
